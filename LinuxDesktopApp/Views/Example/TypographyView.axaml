<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:s="http://smart/avalonia"
             xmlns:local="clr-namespace:LinuxDesktopApp.Views.Example"
             mc:Ignorable="d"
             x:Class="LinuxDesktopApp.Views.Example.TypographyView"
             x:DataType="local:TypographyViewModel"
             s:DataContextResolver.Type="local:TypographyViewModel"
             d:DesignWidth="1920" d:DesignHeight="1080">

    <StackPanel>
        <Label Content="{Binding Message}" />
        <Button Command="{Binding NavigateCommand}"
                Content="Back to Menu" />
    </StackPanel>

</UserControl>

<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:s="http://smart/avalonia"
             xmlns:local="clr-namespace:LinuxDesktopApp.Views.Example"
             mc:Ignorable="d"
             x:Class="LinuxDesktopApp.Views.Example.PrinterView"
             x:DataType="local:PrinterViewModel"
             s:DataContextResolver.Type="local:PrinterViewModel"
             d:DesignWidth="1920" d:DesignHeight="960"
             Height="960">

    <Grid RowDefinitions="60,*">
        <WrapPanel Grid.Row="0" Height="60">
            <Button Command="{Binding PrintTextCommand}"
                    Content="Text"
                    Width="240"
                    Height="60" />
            <Button Command="{Binding PrintImageCommand}"
                    Content="Image"
                    Width="240"
                    Height="60" />
        </WrapPanel>
    </Grid>

</UserControl>

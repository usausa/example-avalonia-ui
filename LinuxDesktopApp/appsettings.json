{
  "Controller": {
    "UseJoystick": true
  },

  "Motor": {
    "Port": "/dev/ttyUSB0"
  },

  "Barcode": {
    "Name": "MJ2818A",
    "Port": "/dev/ttyACM0"
  },

  "Camera": {
    "Device": "/dev/video2",
    "Width": 640,
    "Height": 480,
    "Fps": 15
  },

  "Detect": {
    "Enable": true,
    "Model": "version-RFB-320.onnx",
    "Parallel": false,
    "IntraOpNumThreads": 1,
    "InterOpNumThreads": 1
  },

  "Printer": {
    "LinePrinterDevice": "/dev/usb/lp-thermal",
    "ImagePrinterName": "M110S"
  },

  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "Enrich": [ "FromLogContext", "WithThreadId" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./log/DesktopApp_.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss.fff} {Level:u4} [{ThreadId}] - {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}

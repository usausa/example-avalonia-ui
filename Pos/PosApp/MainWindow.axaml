<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sn="clr-namespace:Smart.Navigation;assembly=Smart.Navigation.Avalonia"
        xmlns:sr="clr-namespace:Smart.Avalonia.Resolver;assembly=Smart.Avalonia"
        xmlns:app="using:PosApp"
        mc:Ignorable="d"
        x:Class="PosApp.MainWindow"
        x:DataType="app:MainWindowViewModel"
        sr:DataContextResolver.Type="app:MainWindowViewModel"
        Title="PosApp"
        Icon="/Assets/avalonia-logo.ico"
        Width="1920" Height="1080"
        d:DesignWidth="1920" d:DesignHeight="1080"
        CanResize="False">

    <Canvas sn:NavigationContainer.Navigator="{Binding Navigator}" />

</Window>
